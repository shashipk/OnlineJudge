<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.infy.eta.databeans.JudgeTestCasesEntity" table="JUDGE_TEST_CASES" schema="JOAMIT">
        <id name="id" column="ID">
            <generator class="sequence-identity">
                <param name="sequence">JUDGE_TEST_CASES_SEQ</param>
            </generator>
        </id>
        <property name="problemId" column="PROBLEM_ID" insert="false" update="false"/>
        <property name="description" column="DESCRIPTION"/>
        <property name="input" column="INPUT"/>
        <property name="output" column="OUTPUT"/>
        <property name="points" column="POINTS"/>
        <many-to-one name="judgeProblemsByProblemId" class="com.infy.eta.databeans.JudgeProblemsEntity">
            <column name="PROBLEM_ID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>